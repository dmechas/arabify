  "devDependencies": {
    "ava": "^0.15.2",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "^6.11.3",
    "babel-register": "^6.11.6",
